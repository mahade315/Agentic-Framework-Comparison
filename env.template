# OpenAI Configuration
OPENAI_API_KEY=your_api_key_here
OPENAI_MODEL=gpt-4o
OPENAI_TEMPERATURE=0.2
OPENAI_MAX_TOKENS=512

# Inference Configuration
NUM_SAMPLES_PER_TASK=10

# Task Selection (Optional)
# TASK_LIMIT=10                              # Run only first N tasks
# TASK_IDS=HumanEval/0,HumanEval/1           # Run specific tasks (comma-separated)
# SHUFFLE_TASKS=false                        # Shuffle task order
# TASK_SHUFFLE_SEED=123                      # Random seed for shuffling

# CrewAI Configuration (Optional)
USE_CREWAI=false                             # Set to 'true' to use CrewAI agents instead of direct API
CREWAI_VERBOSE=false                         # Show detailed agent reasoning
CREWAI_ALLOW_DELEGATION=false                # Allow agents to delegate tasks

# Qwen-Agent Configuration (Optional)
USE_QWEN_AGENT=false                         # Set to 'true' to use Qwen-Agent framework with OpenAI models

# LangChain Configuration (Optional)
USE_LANGCHAIN=false                          # Set to 'true' to use LangChain Agent Executor with OpenAI models

# LangGraph Configuration (Optional)
USE_LANGGRAPH=false                          # Set to 'true' to use LangGraph framework with OpenAI models


