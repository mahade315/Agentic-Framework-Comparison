# OpenAI Configuration
OPENAI_API_KEY=your_api_key_here
OPENAI_MODEL=gpt-4o
OPENAI_TEMPERATURE=0.2
OPENAI_MAX_TOKENS=300

# Inference Configuration
NUM_SAMPLES_PER_TASK=10

# Task Selection (Optional)
# TASK_LIMIT=10                              # Run only first N tasks
# TASK_IDS=HumanEval/0,HumanEval/1           # Run specific tasks (comma-separated)
# SHUFFLE_TASKS=false                        # Shuffle task order
# TASK_SHUFFLE_SEED=42                       # Random seed for shuffling

# Agent Framework Selection (only one should be true)
USE_CREWAI=false                             # Set to 'true' to use CrewAI agents
USE_METAGPT=false                            # Set to 'true' to use MetaGPT agents
USE_TASKWEAVER=false                         # Set to 'true' to use TaskWeaver agents

# CrewAI Configuration (Optional)
CREWAI_VERBOSE=false                         # Show detailed agent reasoning
CREWAI_ALLOW_DELEGATION=false                # Allow agents to delegate tasks

# MetaGPT Configuration (Optional)
METAGPT_MODEL=gpt-4o                         # Model for MetaGPT agent
METAGPT_TEMPERATURE=0.2                      # Temperature for MetaGPT agent

# TaskWeaver Configuration (Optional)
TASKWEAVER_MODEL=gpt-4o                      # Model for TaskWeaver agent
TASKWEAVER_TEMPERATURE=0.2                   # Temperature for TaskWeaver agent



